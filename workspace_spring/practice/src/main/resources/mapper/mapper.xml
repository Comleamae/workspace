<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 해당 파일에 모든 쿼리문을 작성 -->
<mapper namespace="practiceMapper">
    <resultMap id="subject" type="com.green.practice.vo.PracticeVO">
        <result column="" property=""/>
    </resultMap>
	<!--모든 연구자료를 보여주는 쿼리-->
    <select id="getSubjectList" resultMap="subject">
        SELECT SUBJECT_NUM
            , TITLE
            , WORKER
            , REPORT
            , READ_CNT
        FROM SIMPLE_PRACTICE;
    </select>
</mapper>































